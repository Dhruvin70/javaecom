<?xml version="1.0" encoding="UTF-8"?>
<element>
<web-app id="WebApp_ID" version="6.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" >
  <servlet>
        <servlet-name>userRegister</servlet-name>
        <servlet-class>com.user.servlet.AddRegister</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>userRegister</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>userLogin</servlet-name>
        <servlet-class>com.user.servlet.UserLogin</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>userLogin</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>LogoutClient</servlet-name>
        <servlet-class>com.user.servlet.Logout</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LogoutClient</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>AdminAddItems</servlet-name>
        <servlet-class>com.admin.servlet.AdminAddItems</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AdminAddItems</servlet-name>
        <url-pattern>/adminAddItems</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>Delete</servlet-name>
        <servlet-class>com.admin.servlet.DeleteItem</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Delete</servlet-name>
        <url-pattern>/deleteItem</url-pattern>
    </servlet-mapping>
    
    
</web-app>
</element>